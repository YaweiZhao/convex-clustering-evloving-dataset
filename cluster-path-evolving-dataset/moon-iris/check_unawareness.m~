function [is_restart] = check_unawareness( s_hyp)
control_awareness = s_hyp.control_awareness;
Delta = s_hyp.move_data_cluster;
lambda_opt_vec = s_hyp.lambda_opt_vec;
m = s_hyp.m;
d= s_hyp.d;
Q = s_hyp.Q;
[~,num_lambda] = size(lambda_opt_vec);
for i=1:
temp = transpose(reshape(Delta,n*d,1))*transpose(kron(eye(d),Q))*reshape(lambda,m*d,1);
if abs(temp) > control_awareness
    fprintf('[YES] change: %.2f | awareness: %.2f. restart convex clustering\n', abs(temp), control_awareness);
    is_restart = true; 
else
    fprintf('[NO] change: %.2f | awareness: %.2f. continue...\n', abs(temp), control_awareness);
    is_restart = no;
end










end

